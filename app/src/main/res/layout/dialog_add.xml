<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dialog"
    android:layout_width="370dp"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="#FFFFFF"
    android:orientation="vertical">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp">

        <LinearLayout
            android:id="@+id/layout_prod"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginStart="14dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:text="@string/producer_colon"
                android:textColor="@android:color/black"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/edit_prod"
                android:layout_width="140sp"
                android:layout_height="wrap_content"
                tools:ignore="Autofill"
                android:hint="@string/producer"
                android:inputType="text"
                android:maxLength="40"
                android:textSize="14sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/layout_prod"
            android:layout_marginStart="14dp"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <TextView
                android:layout_width="80dp"
                android:layout_height="wrap_content"
                android:text="@string/model_colon"
                android:textColor="@android:color/black"
                android:textSize="16sp" />

            <EditText
                android:id="@+id/edit_model"
                android:layout_width="140sp"
                android:layout_height="wrap_content"
                tools:ignore="Autofill"
                android:hint="@string/model"
                android:maxLength="40"
                android:inputType="text"
                android:textSize="14sp" />
        </LinearLayout>

        <RadioGroup
            android:id="@+id/radio_group"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_toEndOf="@+id/layout_prod">

            <RadioButton
                android:id="@+id/radio_nausznik"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:checked="true"
                android:onClick="onRadioButtonClicked"
                android:text="@string/earmuff"
                android:textSize="16sp" />

            <RadioButton
                android:id="@+id/radio_wkladka"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:onClick="onRadioButtonClicked"
                android:text="@string/inserts"
                android:textSize="16sp" />
        </RadioGroup>
    </RelativeLayout>


    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16sp"
        android:orientation="vertical">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <TextView
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:text="@string/Hz_colon"
                android:textColor="@android:color/black"
                android:textSize="16sp" />

            <TextView
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/number125"
                android:textColor="@android:color/black"
                android:textSize="14sp" />

            <TextView
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/number250"
                android:textColor="@android:color/black"
                android:textSize="14sp" />

            <TextView
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/number500"
                android:textColor="@android:color/black"
                android:textSize="14sp" />

            <TextView
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/number1k"
                android:textColor="@android:color/black"
                android:textSize="14sp" />

            <TextView
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/number2k"
                android:textColor="@android:color/black"
                android:textSize="14sp" />

            <TextView
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/number4k"
                android:textColor="@android:color/black"
                android:textSize="14sp" />

            <TextView
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/number8k"
                android:textColor="@android:color/black"
                android:textSize="14sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <TextView
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:text="@string/mf_colon"
                android:textColor="@android:color/black"
                android:textSize="16sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_mf125"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_mf250"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_mf500"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_mf1000"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_mf2000"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_mf4000"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_mf8000"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <TextView
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:text="@string/apv_colon"
                android:textColor="@android:color/black"
                android:textSize="16sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_apv125"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_apv250"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_apv500"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_apv1000"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_apv2000"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_apv4000"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_apv8000"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/default_number"
                android:inputType="numberDecimal"
                android:maxLength="4"
                android:textSize="14sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <TextView
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/L_colon"
                android:textColor="@android:color/black"
                android:textSize="16sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_L"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/L"
                android:inputType="number"
                android:maxLength="2"
                android:textSize="14sp" />

            <TextView
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/M_colon"
                android:textColor="@android:color/black"
                android:textSize="16sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_M"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/M"
                android:inputType="number"
                android:maxLength="2"
                android:textSize="14sp" />

            <TextView
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/H_colon"
                android:textColor="@android:color/black"
                android:textSize="16sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_H"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/H"
                android:inputType="number"
                android:maxLength="2"
                android:textSize="14sp" />

            <TextView
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/snr_colon"
                android:textColor="@android:color/black"
                android:textSize="16sp" />

            <EditText
                tools:ignore="Autofill"
                android:id="@+id/edit_SNR"
                android:layout_width="36dp"
                android:layout_height="wrap_content"
                android:hint="@string/SNR"
                android:inputType="number"
                android:maxLength="2"
                android:textSize="14sp" />
        </TableRow>
    </TableLayout>
</LinearLayout>